{
	"useTabs": true,
	"singleQuote": true,
	"trailingComma": "all",
	"printWidth": 120,
	"tabWidth": 4,
	"arrowParens": "always",
	"bracketSpacing": true,
	"svelteStrictMode": true,
	"svelteSortOrder": "options-styles-scripts-markup",
	"svelteAllowShorthand": true,
	"svelteIndentScriptAndStyle": true,
	"htmlWhitespaceSensitivity": "strict",
	"semi": true,
	"bracketSameLine": false,
	"jsxSingleQuote": true
}
